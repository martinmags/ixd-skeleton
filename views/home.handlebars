<body onload='startVideo();'>
  <div class="screen_container">
    <!-- Profile Button; Logo; Help ------------------->
    <!-- Include a first-time user message overlay-->
    <div class="top_container"> 
      <a class="profileBtn" href="profile">
        <img id="profileicon" src="images/profileIcon.png">
      </a>
      <header class="title">
        <img id="logo" src="/images/logo.png"/>
      </header>
      <a class="helpBtn" href="help">
        <img id="profileicon" src="images/helpIcon.png">
      </a>
    </div>

    <!-- Camera Component -------------------------->
    <!-- Include a first-time user message overlay-->
    <div class="content_container">
      <video id="camera" autoplay></video>
      <canvas id="canvas"></canvas>
    </div>
</body>
</html>
